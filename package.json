{
  "name": "electron-vue-vite",
  "version": "2.0.0",
  "main": "dist/electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-packager . --overwrite --platform=linux --arch=armv7l --out=out"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "electron-packager": "^16.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vite-plugin-electron": "^0.9.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.13",
    "vue-tsc": "^0.40.1",
    "ws": "^3.1.0",
    "timer.js": "^1.0.4"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOST": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 3344,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "animejs": "^3.2.1"
  }
}
